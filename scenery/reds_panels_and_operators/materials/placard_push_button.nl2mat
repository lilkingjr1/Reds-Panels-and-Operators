          <map>textures/placards/placard_manual_block_mode.png</map>
          <map>textures/placards/placard_manual_brake_release.png</map>
          <map>textures/placards/placard_platform.png</map>
          <map>textures/placards/placard_reset.png</map>
          <map>textures/placards/placard_restraints_lock.png</map>
