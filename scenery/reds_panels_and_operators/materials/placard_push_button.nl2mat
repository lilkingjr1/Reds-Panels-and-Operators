          <map>textures/placards/placard_block_reset.png</map>
          <map>textures/placards/placard_clear_block.png</map>
          <map>textures/placards/placard_dispatch.png</map>
          <map>textures/placards/placard_estop_reset.png</map>
          <map>textures/placards/placard_full_manual_mode.png</map>
          <map>textures/placards/placard_harness_lock.png</map>
          <map>textures/placards/placard_harness_open.png</map>
          <map>textures/placards/placard_horn.png</map>
          <map>textures/placards/placard_jog_lift_button.png</map>
          <map>textures/placards/placard_lamp_test.png</map>
