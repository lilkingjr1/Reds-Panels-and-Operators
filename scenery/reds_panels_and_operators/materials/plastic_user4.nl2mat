<?xml version="1.0" encoding="UTF-8"?>
<root>
  <material>
    <description>Matte plastic that is recolorable using the User 4 custom defined color.</description>
    <renderpass>
      <shadercore>Result = Mul(Entity4, 0.7);
Specular = 0;
Roughness = 1;</shadercore>
      <transparency mode="opaque"/>
      <linearshading/>
      <rgbgen mode="lighting"/>
    </renderpass>
  </material>
</root>