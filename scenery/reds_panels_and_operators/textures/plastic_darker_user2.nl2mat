<?xml version="1.0" encoding="UTF-8"?>
<root>
  <material>
    <description>Matte plastic that is recolorable using the User 2 custom defined color, and is 50% darker.</description>
    <renderpass>
      <shadercore>Result = Mul(Entity2, 0.25);
Specular = 0;
Roughness = 1;</shadercore>
      <transparency mode="opaque"/>
      <linearshading/>
      <rgbgen mode="lighting"/>
    </renderpass>
  </material>
</root>